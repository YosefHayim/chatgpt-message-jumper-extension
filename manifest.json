{
  "manifest_version": 3,
  "name": "ChatGPT Message Jumper",
  "version": "1.1",
  "description": "Jump to previous ChatGPT responses on chat.openai.com.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://chatgpt.com/*"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ]
}
